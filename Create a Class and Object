
public class Student {

    String name;
    int rollNumber;

    void displayDetails() {
        System.out.println("Name: " + name);
        System.out.println("Roll Number: " + rollNumber);
    }

    public static void main(String[] args) {
        Student s1 = new Student();

        s1.name = "palak";
        s1.rollNumber = 11;

        s1.displayDetails();
    }
}
