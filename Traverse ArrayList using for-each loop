import java.util.ArrayList;

public class ArrayListForEach {

    public static void main(String[] args) {

        ArrayList<Integer> list = new ArrayList<>();

        list.add(5);
        list.add(15);
        list.add(25);
        list.add(35);

        for (int num : list) {
            System.out.println(num);
        }
    }
}
