public class Array {
    public static int linearSearch(int[] arr, int target) {
        for(int i = 0; i < arr.length; i++) {
            if(arr[i] == target) return i;
        }
        return -1;
    }

    public static void main(String[] args) {
        int[] arr = {6,7,8,9,4,5};
        int target = 5;
        System.out.println(linearSearch(arr, target));
    }
}
