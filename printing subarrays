 public class Array{
    public static void subarray(int nums[])
{
    for(int i= 0 ; i<nums.length;i++){
        for(int j = i;j<nums.length;j++){
            for(int k = i; k<=j;k++){
System.out.print(nums[k]+ " ");
            }
        }
    }
} public static void main(String[] args) {
    int nums[] = {2,8,3};
    subarray(nums);
}
 }
