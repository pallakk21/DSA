class Shape {
    void area() {
        System.out.println("Area of shape");
    }
}

class Circle extends Shape {
    @Override
    void area() {
        System.out.println("Area of Circle");
    }
}

class Rectangle extends Shape {
    @Override
    void area() {
        System.out.println("Area of Rectangle");
    }
}

public class Main {
    public static void main(String[] args) {
        Shape s;

        s = new Circle();
        s.area();

        s = new Rectangle();
        s.area();
    }
}
